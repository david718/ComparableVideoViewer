{
  "name": "nalbi-airport-detector",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "yarn run build-main && yarn run build-renderer",
    "start-renderer-dev": "webpack-dev-server --mode development --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --mode development --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 yarn run start-renderer-dev",
    "prestart": "yarn run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "precommit": "lint-staged",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "yarn run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "pretty": "prettier --config ./.prettierrc --write src/**/*.{js,ts,tsx,json,css,scss,md}"
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "tslint --project ./tsconfig.json --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "Viewer",
    "appId": "ai.nalbi.jeewangue.airport-detector",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "AppImage"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:jeewangue/nalbi-airport-detector.git"
  },
  "author": {
    "name": "Jee Wan Gue",
    "email": "jeewangue@gmail.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/jest": "^24.0.14",
    "@types/lodash": "^4.14.134",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^12.0.8",
    "@types/react-dom": "^16.8.4",
    "@types/react-grid-layout": "^0.16.7",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.1.0",
    "@types/react-test-renderer": "^16.8.2",
    "@types/redux-logger": "^3.0.7",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^4.1.16",
    "@types/webdriverio": "^5.0.0",
    "@types/webpack-env": "^1.13.3",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^5.1.3",
    "css-loader": "^3.0.0",
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.11.0",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.3",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/pro-light-svg-icons": "^5.9.0",
    "@fortawesome/pro-regular-svg-icons": "^5.9.0",
    "@fortawesome/pro-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^3.0.0",
    "lodash": "^4.17.13",
    "logrocket": "^1.0.0",
    "lowdb": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^7.1.0",
    "react-split-pane": "^0.1.87",
    "recordrtc": "^5.5.5",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "shortid": "^2.2.14",
    "styled-components": "^4.3.1",
    "typeface-roboto": "^0.0.54",
    "wrtc": "^0.4.1"
  }
}
